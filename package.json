        "@types/bcrypt": "^5.0.2",
        "@types/node": "^20.11.5",
        "@types/supertest": "6.0.2",
        "prisma": "^5.8.1",
        "supertest": "6.3.4",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@fastify/cookie": "^9.3.0",
        "@fastify/jwt": "^8.0.0",
        "@prisma/client": "^5.8.1",
        "bcrypt": "^5.1.1",
